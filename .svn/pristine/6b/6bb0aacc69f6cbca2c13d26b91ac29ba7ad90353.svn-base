package com.netcracker.unc.lab3;

/**
 *id_flight number CONSTRAINT  flight_pk_flight_id PRIMARY KEY,
id_route number, -- CONSTRAINT flight_fk_route_id REFERENCES route(Id_route),
id_plane number, --CONSTRAINT flight_fk_plane_id REFERENCES plane(Id),
beg_fly_time date,
ticket_cost number
 * @author Ольга
 */
public class Flight {
  private int idFlight;
  private int idRoute;
  private int idPlane;
  private int begFly;
  private int cost;

    /**
     * @return the idFlight
     */
    public int getIdFlight() {
        return idFlight;
    }

    /**
     * @param idFlight the idFlight to set
     */
    public void setIdFlight(int idFlight) {
        this.idFlight = idFlight;
    }

    /**
     * @return the idRoute
     */
    public int getIdRoute() {
        return idRoute;
    }

    /**
     * @param idRoute the idRoute to set
     */
    public void setIdRoute(int idRoute) {
        this.idRoute = idRoute;
    }

    /**
     * @return the idPlane
     */
    public int getIdPlane() {
        return idPlane;
    }

    /**
     * @param idPlane the idPlane to set
     */
    public void setIdPlane(int idPlane) {
        this.idPlane = idPlane;
    }

    /**
     * @return the begFly
     */
    public int getBegFly() {
        return begFly;
    }

    /**
     * @param begFly the begFly to set
     */
    public void setBegFly(int begFly) {
        this.begFly = begFly;
    }

    /**
     * @return the cost
     */
    public int getCost() {
        return cost;
    }

    /**
     * @param cost the cost to set
     */
    public void setCost(int cost) {
        this.cost = cost;
    }
}
